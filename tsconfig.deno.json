{
  "extends": "./tsconfig.json",
  "include": ["dist-deno"],
  "exclude": [],
  "compilerOptions": {
    "rootDir": "./dist-deno",
    "lib": ["es2020", "DOM"],
    "paths": {
      "@wallcrawler/sdk/_shims/auto/*": ["deno/_shims/auto/*-deno"],
      "@wallcrawler/sdk/*": ["deno/*"],
      "@wallcrawler/sdk": ["deno/index.ts"]
    },
    "noEmit": true,
    "declaration": true,
    "declarationMap": true,
    "outDir": "dist-deno",
    "pretty": true,
    "sourceMap": true
  }
}
